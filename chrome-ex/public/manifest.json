{
  "manifest_version": 3,
  "name": "Vocab",
  "author": "Iris",
  "version": "1.0.0",
  "description": "Replace new tab screen with English words",
  "action": {}, 
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/static/js/content.js"
      ]
    }
  ],
  "icons": {
    "16": "icon16.ico",
    "48": "icon48.ico",
    "128": "icon128.ico"
  }, 
"permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "contextMenus",
    "storage",
    "identity",
    "notifications"
],
"web_accessible_resources": [{
  "resources": ["index.html", "/static/js/content.js" ],
  "matches": ["<all_urls>"],
  "extension_ids": ["kbjambaljfpmbadpgmclckcfolhpliea"]
}]
}